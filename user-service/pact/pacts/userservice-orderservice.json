{
  "consumer": {
    "name": "UserService"
  },
  "provider": {
    "name": "OrderService"
  },
  "interactions": [
    {
      "description": "upon",
      "providerState": "state",
      "request": {
        "method": "GET",
        "path": "/users/hoon",
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json;"
        },
        "body": {
          "message": "ok",
          "data": {
            "userId": "hoon",
            "orders": [
              {
                "id": 1,
                "title": "product#1",
                "price": 3000
              }
            ]
          }
        },
        "matchingRules": {
          "$.body.data.userId": {
            "match": "type"
          },
          "$.body.data.orders": {
            "min": 1
          },
          "$.body.data.orders[*].*": {
            "match": "type"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}